{
    "family": "arb-bot",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::538602529242:role/aws-service-role/ecs.amazonaws.com/AWSServiceRoleForECS",
    "containerDefinitions": [
        {
            "name": "arb-bot",
            "image": "538602529242.dkr.ecr.eu-west-1.amazonaws.com/arb-bot:0.01",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}